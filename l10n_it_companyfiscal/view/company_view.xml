<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="view_company_fiscal_form" model="ir.ui.view">
            <field name="name">res.company.fiscal.form</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form"/>
            <field name="arch" type="xml">
                <field name="company_registry" position="after">
                    <field name="capitale_sociale"/>
                    <field name="socio_unico"/>
                    <field name="stato_liquidazione"/>
                    <field name="regime_fiscale"/>
                </field>
            </field>
        </record>

    </data>
</openerp>
